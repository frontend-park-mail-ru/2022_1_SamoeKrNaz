{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": [1, "tab"],
        "no-tabs": 0,
        "no-unused-vars": 0,
        "max-len": [1, 500]
    }
}
