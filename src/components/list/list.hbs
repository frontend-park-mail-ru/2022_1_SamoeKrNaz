<div class="desk__globalTitle">
    <span>{{board.title}}</span>

    <div class="desk__globalTitle-avatars">
        {{#each addUsers}}
            <div class="taskBlock__img-block" data-id="{{ idu }}">
                <div class="taskBlock__img-popover">{{ username }}</div>
                <img alt="{{ username }} avatar" src="../{{ img_avatar }}" data-id="{{ idu }}" class="taskBlock__img">
                <div data-id="{{ idu }}" class="taskBlock__img-block-delete">
                    <svg data-id="{{ idu }}" width="40" height="40" viewBox="0 0 40 40" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <circle data-id="{{ idu }}" cx="20" cy="20" r="20" fill="#2D2D2D" fill-opacity="0.4"/>
                        <path data-id="{{ idu }}" d="M15.1969 14.7406C15.4897 14.4478 15.9646 14.4478 16.2575 14.7406L20.5002 18.9833L24.7428 14.7406C25.0357 14.4478 25.5106 14.4478 25.8035 14.7406C26.0964 15.0335 26.0964 15.5084 25.8035 15.8013L21.5608 20.0439L25.8035 24.2866C26.0964 24.5795 26.0964 25.0544 25.8035 25.3472C25.5106 25.6401 25.0357 25.6401 24.7428 25.3472L20.5002 21.1046L16.2575 25.3472C15.9646 25.6401 15.4897 25.6401 15.1969 25.3472C14.904 25.0544 14.904 24.5795 15.1969 24.2866L19.4395 20.0439L15.1969 15.8013C14.904 15.5084 14.904 15.0335 15.1969 14.7406Z" fill="#F8FAFD"/>
                    </svg>
                </div>
            </div>
        {{/each}}
    </div>
</div>

{{> buttonsList/buttonsList }}

<div class="column">
    {{#each board.Lists}}
        <div class="desk__column-block" data-dnd="true">
            <div class="desk desk__column" data-position="{{ position }}" data-id="{{ idl }}">
                <div class="desk__column-title">
                    <input data-id="{{idl}}" class="desk__title-input header2" type="text" value="{{title}}">
                    <svg class="desk__listIcon" data-id="{{idl}}"  width="14" height="16" viewBox="0 0 14 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path data-id="{{idl}}" d="M1.92326 11.9188C2.05648 13.1846 2.1231 13.8175 2.4126 14.2954C2.6676 14.7163 3.04118 15.0527 3.48653 15.2622C3.99212 15.5001 4.6285 15.5001 5.90128 15.5001H8.09918C9.37193 15.5001 10.0083 15.5001 10.5139 15.2622C10.9592 15.0527 11.3328 14.7164 11.5878 14.2954C11.8773 13.8175 11.944 13.1846 12.0772 11.9189L12.6073 6.88376C12.6395 6.57769 12.6556 6.42466 12.6058 6.3063C12.5622 6.20233 12.4846 6.11619 12.3858 6.06186C12.2733 6 12.1194 6 11.8117 6H2.18891C1.88116 6 1.72729 6 1.61478 6.06186C1.51597 6.11619 1.43842 6.20233 1.39473 6.30629C1.34499 6.42465 1.3611 6.57768 1.39331 6.88374L1.92326 11.9188Z" fill="#7F7F7F"/>
                        <path data-id="{{idl}}" d="M1 4.5H13C13.5523 4.5 14 4.05229 14 3.50002C14 2.94772 13.5523 2.5 13 2.5H9.64648C9.32002 1.34575 8.25878 0.5 7 0.5C5.74122 0.5 4.67998 1.34575 4.35352 2.5H1C0.447715 2.5 0 2.94772 0 3.5C0 4.05228 0.447715 4.5 1 4.5Z" fill="#7F7F7F"/>
                    </svg>
                </div>
                {{#each Tasks}}
                    <div class="desk__task-block">
                        <div class="desk__task" data-isDnd="true" data-id="{{idt}}" data-idl="{{idl}}" data-position="{{position}}">
                            <div data-id="{{idt}}" class="desk__task-text">
                                {{title}}
                            </div>
                            <div data-id="{{idt}}" class="desk__task-close">
                                <svg data-id="{{idt}}" width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path data-id="{{idt}}" fill-rule="evenodd" clip-rule="evenodd" d="M8 14.5C11.5899 14.5 14.5 11.5899 14.5 8C14.5 4.41015 11.5899 1.5 8 1.5C4.41015 1.5 1.5 4.41015 1.5 8C1.5 11.5899 4.41015 14.5 8 14.5ZM8 16C12.4183 16 16 12.4183 16 8C16 3.58172 12.4183 0 8 0C3.58172 0 0 3.58172 0 8C0 12.4183 3.58172 16 8 16Z" fill="#7F7F7F"/>
